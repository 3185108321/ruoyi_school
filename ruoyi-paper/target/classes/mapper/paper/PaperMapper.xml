<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.paper.mapper.PaperMapper">
    
    <resultMap type="Paper" id="PaperResult">
        <result property="paperId"    column="paper_id"    />
        <result property="title"    column="title"    />
        <result property="keywords"    column="keywords"    />
        <result property="abstractText"    column="abstract_text"    />
        <result property="publishDate"    column="publish_date"    />
        <result property="journal"    column="journal"    />
        <result property="doi"    column="doi"    />
        <result property="url"    column="url"    />
        <result property="filePath"    column="file_path"    />
        <result property="userId"    column="user_id"    />
        <result property="deptId"    column="dept_id"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
        <result property="delFlag"    column="del_flag"    />
    </resultMap>

    <sql id="selectPaperVo">
        select paper_id, title, keywords, abstract_text, publish_date, journal, doi, url, file_path, user_id, dept_id, create_by, create_time, update_by, update_time, remark, del_flag from biz_paper
    </sql>

    <select id="selectPaperList" parameterType="Paper" resultMap="PaperResult">
        <include refid="selectPaperVo"/>
        <where>  
            <if test="title != null  and title != ''"> and title = #{title}</if>
            <if test="keywords != null  and keywords != ''"> and keywords = #{keywords}</if>
            <if test="abstractText != null  and abstractText != ''"> and abstract_text = #{abstractText}</if>
            <if test="publishDate != null "> and publish_date = #{publishDate}</if>
            <if test="journal != null  and journal != ''"> and journal = #{journal}</if>
            <if test="doi != null  and doi != ''"> and doi = #{doi}</if>
            <if test="url != null  and url != ''"> and url = #{url}</if>
            <if test="filePath != null  and filePath != ''"> and file_path = #{filePath}</if>
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="deptId != null "> and dept_id = #{deptId}</if>
        </where>
    </select>
    
    <select id="selectPaperByPaperId" parameterType="Long" resultMap="PaperResult">
        <include refid="selectPaperVo"/>
        where paper_id = #{paperId}
    </select>

    <insert id="insertPaper" parameterType="Paper" useGeneratedKeys="true" keyProperty="paperId">
        insert into biz_paper
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="title != null and title != ''">title,</if>
            <if test="keywords != null">keywords,</if>
            <if test="abstractText != null">abstract_text,</if>
            <if test="publishDate != null">publish_date,</if>
            <if test="journal != null">journal,</if>
            <if test="doi != null">doi,</if>
            <if test="url != null">url,</if>
            <if test="filePath != null">file_path,</if>
            <if test="userId != null">user_id,</if>
            <if test="deptId != null">dept_id,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
            <if test="delFlag != null">del_flag,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="title != null and title != ''">#{title},</if>
            <if test="keywords != null">#{keywords},</if>
            <if test="abstractText != null">#{abstractText},</if>
            <if test="publishDate != null">#{publishDate},</if>
            <if test="journal != null">#{journal},</if>
            <if test="doi != null">#{doi},</if>
            <if test="url != null">#{url},</if>
            <if test="filePath != null">#{filePath},</if>
            <if test="userId != null">#{userId},</if>
            <if test="deptId != null">#{deptId},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
            <if test="delFlag != null">#{delFlag},</if>
         </trim>
    </insert>

    <update id="updatePaper" parameterType="Paper">
        update biz_paper
        <trim prefix="SET" suffixOverrides=",">
            <if test="title != null and title != ''">title = #{title},</if>
            <if test="keywords != null">keywords = #{keywords},</if>
            <if test="abstractText != null">abstract_text = #{abstractText},</if>
            <if test="publishDate != null">publish_date = #{publishDate},</if>
            <if test="journal != null">journal = #{journal},</if>
            <if test="doi != null">doi = #{doi},</if>
            <if test="url != null">url = #{url},</if>
            <if test="filePath != null">file_path = #{filePath},</if>
            <if test="userId != null">user_id = #{userId},</if>
            <if test="deptId != null">dept_id = #{deptId},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
        </trim>
        where paper_id = #{paperId}
    </update>

    <delete id="deletePaperByPaperId" parameterType="Long">
        delete from biz_paper where paper_id = #{paperId}
    </delete>

    <delete id="deletePaperByPaperIds" parameterType="String">
        delete from biz_paper where paper_id in 
        <foreach item="paperId" collection="array" open="(" separator="," close=")">
            #{paperId}
        </foreach>
    </delete>
</mapper>